#: interface.cpp:313
#, kde-format
msgid "Scanning stopped by user."
msgstr "Digitalização parada pelo usuário."

#: options/batchdelayoption.cpp:31
#, kde-format
msgid "Batch mode time delay"
msgstr "Atraso do modo em lote"

#: options/batchdelayoption.cpp:36
#, kde-format
msgid "Specify the time delay between each scan when batch mode is enabled."
msgstr ""
"Especifique o atraso entre cada digitalização quando o modo em lote estiver "
"habilitado."

#: options/batchmodeoption.cpp:31
#, kde-format
msgid "Batch mode with time delay"
msgstr "Modo em lote com atraso de tempo"

#: options/batchmodeoption.cpp:36
#, kde-format
msgid ""
"Enables batch mode scanning. Continues scanning after a delay until canceled."
msgstr ""
"Habilita a digitalização em lote. Continua a digitalização após um atraso "
"até ser cancelado."

#: options/invertoption.cpp:58
#, kde-format
msgid "Invert colors"
msgstr "Inverter cores"

#: options/invertoption.cpp:63
#, kde-format
msgid "Invert the colors of the scanned image."
msgstr "Inverter as cores da imagem digitalizada."

#: options/pagesizeoption.cpp:110
#, kde-format
msgctxt "Page size landscape"
msgid "Landscape %1"
msgstr "Paisagem %1"

#: options/pagesizeoption.cpp:189
#, kde-format
msgid "Scan area size"
msgstr "Tamanho da área de digitalização"

#: options/pagesizeoption.cpp:194
#, kde-format
msgid "Select a predefined page size for the scanning area."
msgstr ""
"Selecionar um tamanho de página pré-definido para a área de digitalização."
