#: interface.cpp:313
#, kde-format
msgid "Scanning stopped by user."
msgstr "Scannen is gestopt door gebruiker."

#: options/batchdelayoption.cpp:31
#, kde-format
msgid "Batch mode time delay"
msgstr "Tijdsvertraging van modus bulk"

#: options/batchdelayoption.cpp:36
#, kde-format
msgid "Specify the time delay between each scan when batch mode is enabled."
msgstr ""
"De tijdsvertraging specificeren tussen elke scan wanneer modus bulk is "
"ingeschakeld."

#: options/batchmodeoption.cpp:31
#, kde-format
msgid "Batch mode with time delay"
msgstr "Modus bulk met tijdsvertraging"

#: options/batchmodeoption.cpp:36
#, kde-format
msgid ""
"Enables batch mode scanning. Continues scanning after a delay until canceled."
msgstr ""
"Schakelt scannen in modus bulk in. Gaat door met scannen na een vertraging "
"tot annuleren."

#: options/invertoption.cpp:58
#, kde-format
msgid "Invert colors"
msgstr "Kleuren omkeren"

#: options/invertoption.cpp:63
#, kde-format
msgid "Invert the colors of the scanned image."
msgstr "De kleuren omkeren van de gescande afbeelding."

#: options/pagesizeoption.cpp:110
#, kde-format
msgctxt "Page size landscape"
msgid "Landscape %1"
msgstr "Landschap %1"

