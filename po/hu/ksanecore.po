#: interface.cpp:313
#, kde-format
msgid "Scanning stopped by user."
msgstr "A felhasználó leállította a beolvasást."

#: options/batchdelayoption.cpp:31
#, kde-format
msgid "Batch mode time delay"
msgstr "Kötegelt mód időkésleltetése"

#: options/batchdelayoption.cpp:36
#, kde-format
msgid "Specify the time delay between each scan when batch mode is enabled."
msgstr "Megadja a beolvasások közötti időbeli késleltetés kötegelt mód esetén."

#: options/batchmodeoption.cpp:31
#, kde-format
msgid "Batch mode with time delay"
msgstr "Kötegelt mód időkésleltetéssel"

#: options/batchmodeoption.cpp:36
#, kde-format
msgid ""
"Enables batch mode scanning. Continues scanning after a delay until canceled."
msgstr ""
"Bekapcsolja a kötegelt beolvasást. Késleltetve folytatja a beolvasást "
"megszakításig."

#: options/invertoption.cpp:58
#, kde-format
msgid "Invert colors"
msgstr "Színek invertálása"

#: options/invertoption.cpp:63
#, kde-format
msgid "Invert the colors of the scanned image."
msgstr "A beolvasott kép színeinek invertálása."

#: options/pagesizeoption.cpp:124
#, kde-format
msgid "Scan area size"
msgstr "Beolvasóterület mérete"

#: options/pagesizeoption.cpp:129
#, kde-format
msgid "Select a predefined page size for the scanning area."
msgstr ""
"Válassza ki a beolvasási terület méretét az előre meghatározott oldalméretek "
"közül."

#: options/pagesizeoption.cpp:265
#, kde-format
msgctxt "Page size landscape"
msgid "Landscape %1"
msgstr "%1 fekvő"
