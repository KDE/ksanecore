#: interface.cpp:313
#, kde-format
msgid "Scanning stopped by user."
msgstr "Skenēšanu apturēja lietotājs."

#: options/batchdelayoption.cpp:31
#, kde-format
msgid "Batch mode time delay"
msgstr "Pakešapstrādes režīma laika noildze"

#: options/batchdelayoption.cpp:36
#, kde-format
msgid "Specify the time delay between each scan when batch mode is enabled."
msgstr ""
"Norādiet laika noildze starp katru skenēšanu, kad ir ieslēgts pakešapstrādes "
"režīms"

#: options/batchmodeoption.cpp:31
#, kde-format
msgid "Batch mode with time delay"
msgstr "Pakešapstrādes režīms ar laika noildzi"

#: options/batchmodeoption.cpp:36
#, kde-format
msgid ""
"Enables batch mode scanning. Continues scanning after a delay until canceled."
msgstr ""
"Ieslēdz skenēšanu pakešapstrādes režīmā. Turpina skenēt ik pēc noteikta "
"laika līdz skenēšanu atceļ."

#: options/invertoption.cpp:58
#, kde-format
msgid "Invert colors"
msgstr "Invertēt krāsas"

#: options/invertoption.cpp:63
#, kde-format
msgid "Invert the colors of the scanned image."
msgstr "Invertē skenētā attēla krāsas."

#: options/pagesizeoption.cpp:119
#, kde-format
msgid "Scan area size"
msgstr "Skenēšanas reģiona izmērs"

#: options/pagesizeoption.cpp:124
#, kde-format
msgid "Select a predefined page size for the scanning area."
msgstr "Atlasiet iepriekš definētu lapas izmēru skenēšanas reģionam."

#: options/pagesizeoption.cpp:260
#, kde-format
msgctxt "Page size landscape"
msgid "Landscape %1"
msgstr "Ainava %1"
